{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "docker:build": "docker-compose up -d",
    "build:nexus-typegen": "ts-node --transpile-only ./src/modules/graphql/schema.ts",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "predev": "yarn run docker:build && yarn run build:nexus-typegen && yarn run gen-env",
    "watch": "tsc -w",
    "build": "tsc",
    "dev": "nodemon dist/index.js",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio",
    "start": "tsc && node dist/index.js",
    "test": "NODE_ENV=test && jest --config jest.config.ts ./__tests__/*.test.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "apollo-server-express": "^3.9.0",
    "argon2": "^0.28.5",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "graphql": "^15.3.0",
    "graphql-middleware": "^6.1.31",
    "graphql-shield": "^7.5.0",
    "ioredis": "^5.1.0",
    "nanoid": "^3.0.0",
    "nexus": "^1.3.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/node": "^18.0.1",
    "gen-env-types": "^1.3.4",
    "nodemon": "^2.0.18",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "schema": "./src/modules/prisma/schema.prisma"
  }
}